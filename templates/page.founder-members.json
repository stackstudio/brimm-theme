      "disabled": true,
      "settings": {
        "title": "OUR TEAM",
        "no-gap": false,
        "first_col": false,
        "text_block": "This isn't our first Rodeo. Each team member brings a unique perspective, experience and set of skills to BRiMM.",
        "heading_size": "h1",
        "image_width": "full",
        "image_ratio": "adapt",
        "columns_desktop": 4,
        "column_alignment": "left",
        "background_style": "none",
